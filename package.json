{
  "name": "blibla-template",
  "version": "0.1.0",
  "private": true,
  "license": "AGPL 3.0",
  "scripts": {
    "prebuild": "npm run favicons",
    "build": "next build",
    "predev": "npm run favicons",
    "dev": "next dev",
    "docker:dev": "docker run --name blibla-template -p 3456:5432  -e POSTGRES_USER=user -e POSTGRES_PASSWORD=password  -e POSTGRES_DB=database -d postgres",
    "eslint": "eslint \"{**/*,*}{.,.config.,.test.,.e2e.}{js,mjs,ts,tsx}\"",
    "favicons": "node scripts/favicons.mjs",
    "lint": "next lint",
    "ncu": "npx npm-check-updates -u",
    "prepare": "husky install",
    "prisma:client": "dotenv -e .env.local -- prisma db push && prisma generate",
    "prisma:format": "prisma format",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:studio": "dotenv -e .env.local -- prisma studio",
    "spj": "npx sort-package-json",
    "start": "next start",
    "test": "npm run test:jest && npm run test:bdd",
    "test:all": "npx cucumber-js features/**/*.feature",
    "test:bdd": "npx cucumber-js",
    "test:ci": "start-server-and-test start http://127.0.0.1:3000 test:bdd",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:only": "npx cucumber-js --tags @only",
    "toc": "npx markdown-toc README.md -i",
    "tsc:noEmit": "tsc --noEmit",
    "vercel-build": "npm run favicons && prisma generate && prisma migrate deploy && next build"
  },
